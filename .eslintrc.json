{
  "extends": ["next/core-web-vitals", "plugin:tailwindcss/recommended", "prettier"],
  "rules": {
    "tailwindcss/no-custom-classname": [
      "error",
      {
        "whitelist": ["post"]
      }
    ],
    "import/order": [
      "error",
      {
        "alphabetize": {
          "order": "asc"
        }
      }
    ]
  }
}
