---
import Layout from '@layouts/Layout.astro';

interface Props {
  frontmatter: {
    title: string;
    description?: string;
  };
}
const { frontmatter } = Astro.props;
---

<Layout title={`${frontmatter.title} | Kengo`} description={frontmatter.description}>
  <article class='prose mt-6'>
    <slot />
  </article>
</Layout>
